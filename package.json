{
  "name": "rakamin-compiler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:create": "cd ./database && sequelize-cli db:create",
    "db:drop": "cd ./database && sequelize-cli db:drop",
    "db:migrate": "cd ./database && sequelize-cli db:migrate",
    "db:seed": "cd ./database && sequelize-cli db:seed:all",
    "add:migration": "cd ./database && sequelize-cli migration:generate --name",
    "add:seeders": "cd ./database && sequelize-cli seed:generate --name"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.6.0",
    "axios": "^1.6.2",
    "framer-motion": "^10.16.5",
    "next": "14.0.2",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "sequelize": "^6.35.0",
    "sequelize-cli": "^6.6.2",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.2",
    "postcss": "^8",
    "tailwindcss": "^3.3.0"
  }
}
